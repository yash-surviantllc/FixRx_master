{
  "name": "fixrxmobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && expo start -c",
    "android": "set NODE_OPTIONS=--openssl-legacy-provider && expo start --android -c",
    "ios": "set NODE_OPTIONS=--openssl-legacy-provider && expo start --ios -c",
    "web": "set NODE_OPTIONS=--openssl-legacy-provider && expo start --web -c",
    "clean": "rmdir /s /q node_modules && rmdir /s /q .expo && del /f /q package-lock.json",
    "reset": "npm run clean && npm install && expo start -c"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.1.1",
    "@expo/vector-icons": "^13.0.0",
    "@expo/webpack-config": "~19.0.1",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-community/slider": "^4.4.2",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "expo": "~49.0.21",
    "expo-auth-session": "~5.0.2",
    "expo-crypto": "~12.4.1",
    "expo-font": "~11.4.0",
    "expo-image-picker": "~14.3.2",
    "expo-linear-gradient": "~12.3.0",
    "expo-status-bar": "~1.6.0",
    "expo-web-browser": "~12.3.2",
    "lucide-react-native": "^0.105.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "socket.io-client": "^4.7.5",
    "react-hook-form": "^7.43.9",
    "react-native": "0.72.10",
    "react-native-reanimated": "~3.5.4",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.1",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@types/react": "~18.2.0",
    "@types/react-dom": "~18.0.10",
    "@types/react-native": "^0.73.0",
    "babel-preset-expo": "~9.5.2",
    "cross-env": "^7.0.3",
    "typescript": "^5.1.3"
  },
  "private": true
}
