{
  "name": "fixrxmobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && expo start -c",
    "android": "set NODE_OPTIONS=--openssl-legacy-provider && expo start --android -c",
    "ios": "set NODE_OPTIONS=--openssl-legacy-provider && expo start --ios -c",
    "web": "set NODE_OPTIONS=--openssl-legacy-provider && expo start --web -c",
    "clean": "rmdir /s /q node_modules && rmdir /s /q .expo && del /f /q package-lock.json",
    "reset": "npm run clean && npm install && expo start -c",
    "setup": "node setup-env.js"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/slider": "5.0.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "expo": "~54.0.0",
    "expo-auth-session": "~7.0.8",
    "expo-crypto": "~15.0.7",
    "expo-font": "~14.0.9",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-web-browser": "~15.0.8",
    "lucide-react-native": "^0.546.0",
    "prop-types": "^15.8.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.43.9",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "react-refresh": "^0.18.0",
    "socket.io-client": "^4.7.5"
  },
  "devDependencies": {
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@types/react": "~19.1.10",
    "@types/react-dom": "~19.1.7",
    "babel-preset-expo": "~54.0.0",
    "cross-env": "^7.0.3",
    "react-native-svg-transformer": "^1.5.1",
    "typescript": "^5.1.3"
