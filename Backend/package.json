  "author": "FixRx Development Team",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "auth0": "^5.0.0",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.11.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "express-validator": "^7.2.1",
    "google-auth-library": "^10.4.0",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
