{
  "name": "fixrx-backend",
  "version": "1.0.0",
  "description": "FixRx Client-Vendor Management Platform Backend API",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "node test-runner.js",
    "test:unit": "jest --testMatch='**/*.test.js'",
    "test:integration": "node test-integration-runner.js",
    "test:architecture": "node test-architecture.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:architecture",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "build": "tsc",
    "migrate": "node src/scripts/migrate.js",
    "seed": "node src/scripts/seed.js"
  },
  "keywords": [
    "client-vendor",
    "management",
    "platform",
    "react-native",
    "nodejs",
    "postgresql"
